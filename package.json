{
  "name": "vite_bundler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "commit": "cz",
    "check": "stylelint && eslint src/",
    "cypress:open": "cypress open",
    "cypress": "cypress run",
    "dev": "vite",
    "deploy": "gh-pages -d dist",
    "eslint": "eslint src/ --fix",
    "lint": "npm run stylelint && npm run eslint && npm run prettier",
    "predeploy": "npm run build",
    "prepare": "husky",
    "prettier": "prettier \"./src/**/*.{js,jsx,ts,tsx,json,html,css,md}\" --write",
    "preview": "vite preview",
    "stylelint": "stylelint \"./src/**/*.css\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@krgaa/react-developer-burger-ui-components": "^1.0.1",
    "@reduxjs/toolkit": "^2.8.2",
    "core-js": "^3.43.0",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/browser": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "commitizen": "^4.3.1",
    "cypress": "^14.4.1",
    "cypress-vite": "^1.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.14.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-readable-classnames": "^1.5.2",
    "vite-plugin-sass-dts": "^1.3.31",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "./src/**/*.css": [
      "stylelint --fix",
      "prettier --write --parser css"
    ],
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --no-warn-ignored",
      "prettier --write --parser typescript"
    ],
    "./src/**/*.{json,html,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
